{
  "name": "gizmosql-ui",
  "version": "2.0.0",
  "description": "GizmoSQL UI - A web-based SQL interface for GizmoSQL servers",
  "private": true,
  "bin": "launcher.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "bundle": "pnpm build && node scripts/bundle.js",
    "package": "pnpm package:prepare &&  cd .next/standalone && npx @yao-pkg/pkg .",
    "package:prepare": "pnpm build && cp -r public .next/standalone/ && cp scripts/launcher.js .next/standalone/ && cp -r .next/static .next/standalone/.next/ && cd .next/standalone && sed -i.bak 's/process.chdir(__dirname)//' server.js && rm server.js.bak && chmod +x launcher.js",
    "package:macos": "pnpm package:prepare && cd .next/standalone && npx @yao-pkg/pkg . -t node22-macos-arm64",
    "package:linux": "pnpm package:prepare && cd .next/standalone && npx @yao-pkg/pkg . -t node22-linux-x64",
    "package:windows": "pnpm package:prepare && cd .next/standalone && npx @yao-pkg/pkg . -t node22-win-x64",
    "clean": "rm -rf dist .next node_modules"
  },
  "keywords": [
    "gizmosql",
    "gizmodata",
    "sql",
    "database",
    "arrow",
    "flight",
    "nextjs"
  ],
  "author": "GizmoData LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@gizmodata/gizmosql-client": "^1.2.10",
    "@monaco-editor/react": "^4.7.0",
    "apache-arrow": "^21.1.0",
    "lucide-react": "^0.562.0",
    "next": "^14.2.35",
    "parquet-wasm": "^0.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/node": "^22.19.6",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@yao-pkg/pkg": "^6.11.0",
    "typescript": "^5.9.3"
  },
  "pkg": {
    "assets": [
      ".next/**/*",
      "public/**/*.*"
    ],
    "targets": [
      "node22-macos-arm64",
      "node22-linux-x64",
      "node22-win-x64"
    ],
    "outputPath": "../../dist"
  }
}
